$("ul.errors").html("")
<% if @webpage_style.errors.any? %>
  <% @webpage_style.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".webpage-style-index").html("<%= escape_javascript(render 'index') %>")
  $("#webpage-style-modal").modal("hide")
<% end %>
